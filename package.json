{
  "name": "cnn-hapi",
  "version": "1.2.0",
  "description": "This is a generic Hapi server for starting projects.",
  "main": "init.js",
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "scripts": {
    "create-release": "tools/create-release.sh",
    "create-release-dry": "tools/create-release.sh --dry-run",
    "generate-authors": "tools/generate-authors.sh",
    "generate-changelog": "changelog-maker --group",
    "generate-docs": "rm -rf docs && esdoc -c .esdoc.json",
    "test": "eslint . && test/validate-json-files.sh",
    "update-apply": "ncu -u",
    "update-check": "ncu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnnlabs/cnn-hapi.git"
  },
  "bugs": {
    "url": "https://github.com/cnnlabs/cnn-hapi/issues"
  },
  "homepage": "https://github.com/cnnlabs/cnn-hapi#readme",
  "engines": {
    "node": ">=4.3.1"
  },
  "dependencies": {
    "boom": "4.2.0",
    "cnn-health": "0.1.3",
    "cnn-metrics": "0.3.10",
    "debounce": "1.0.0",
    "debug": "2.3.3",
    "engine.io": "1.8.1",
    "good": "7.1.0",
    "good-console": "6.3.1",
    "good-squeeze": "5.0.1",
    "handlebars": "4.0.6",
    "hapi": "16.0.1",
    "hapi-swagger": "7.4.0",
    "inert": "4.0.3",
    "joi": "10.0.2",
    "lodash": "4.17.2",
    "socket.io": "1.7.1",
    "vision": "4.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "changelog-maker": "2.2.4",
    "esdoc": "0.4.8",
    "eslint": "3.11.1",
    "jsonlint": "1.6.2",
    "npm-check-updates": "2.8.7"
  },
  "keywords": [
    "cnn",
    "cnnlabs",
    "hapi"
  ],
  "license": "MIT"
}
