{
  "name": "cnn-hapi",
  "version": "2.3.0",
  "description": "This is a generic Hapi server for starting projects.",
  "main": "init.js",
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "scripts": {
    "create-release": "tools/create-release.sh",
    "create-release-dry": "tools/create-release.sh --dry-run",
    "generate-authors": "tools/generate-authors.sh",
    "generate-changelog": "changelog-maker --group",
    "generate-docs": "rm -rf docs && esdoc -c .esdoc.json",
    "test": "eslint . && test/validate-json-files.sh",
    "update-apply": "ncu -u",
    "update-check": "ncu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnnlabs/cnn-hapi.git"
  },
  "bugs": {
    "url": "https://github.com/cnnlabs/cnn-hapi/issues"
  },
  "homepage": "https://github.com/cnnlabs/cnn-hapi#readme",
  "engines": {
    "node": ">=4.3.1"
  },
  "dependencies": {
    "cnn-health": "0.2.0",
    "cnn-metrics": "0.5.0",
    "good": "7.2.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.2",
    "hapi": "16.4.3",
    "hapi-swagger": "7.7.0",
    "inert": "4.2.0",
    "lodash": "4.17.4",
    "vision": "4.1.1"
  },
  "devDependencies": {
    "changelog-maker": "2.2.5",
    "esdoc": "0.5.2",
    "eslint": "4.0.0",
    "jsonlint": "1.6.2",
    "npm-check-updates": "2.11.3"
  },
  "keywords": [
    "cnn",
    "cnnlabs",
    "hapi"
  ],
  "license": "MIT"
}
